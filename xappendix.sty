\ProvidesExplPackage {xappendix} {2014/04/01} {0.0}
  {An extensible appendix package}
\NewDocumentCommand\appendixset{O{default}}{
    \pgfqkeys{/xappendix/#1}
  }
\RenewDocumentCommand\appendix{O{default}}{
    \@@_apply_settings{#1}
    \@@_old_appendix
  }

